{
  "name": "grow-your-need-saas-school",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:vector-db\" \"npm run dev:check-ollama\" --names \"FRONTEND,VECTOR-DB,OLLAMA\" --prefix-colors \"cyan,green,yellow\"",
    "dev:frontend": "vite",
    "dev:vector-db": "cd database && python web-interface.py",
    "dev:check-ollama": "node scripts/check-ollama.js",
    "dev:frontend-only": "vite",
    "build": "vite build",
    "build:analyze": "vite build && open dist/bundle-analysis.html",
    "build:stats": "vite build --mode production",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "test:update": "jest --updateSnapshot",
    "test:chunks": "npm run build && node scripts/analyze-chunks.js",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.23",
    "@google/genai": "^1.11.0",
    "@pimzino/agentic-tools-mcp": "^1.8.1",
    "ai": "^4.3.19",
    "dotenv": "^17.2.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^3.1.0",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^22.17.0",
    "@types/react": "^19.1.9",
    "@types/testing-library__jest-dom": "^5.14.9",
    "canvas": "^3.1.2",
    "concurrently": "^9.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^3.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "terser": "^5.43.1",
    "ts-jest": "^29.4.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-bundle-analyzer": "^1.1.0"
  }
}
